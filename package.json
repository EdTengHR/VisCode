{
	"name": "viscode",
	"displayName": "VisCode",
	"description": "An extension for visualizing your code",
	"publisher": "EdmundTeng",
	"author": {
		"name": "Edmund Teng"
	},
	"homepage": "https://github.com/EdTengHR/VisCode",
	"repository": {
		"type": "git",
		"url": "https://github.com/EdTengHR/VisCode"
	},
	"bugs": {
		"url": "https://github.com/EdTengHR/VisCode/issues"
	},
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"keywords": [
		"Visualize",
		"Code visualizer"
	],
	"icon": "images/logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:viscode.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "viscode.start",
				"title": "Visualize current code"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@types/d3": "^7.1.0",
		"d3": "^7.1.0",
		"node-fetch": "^3.0.0"
	}
}
